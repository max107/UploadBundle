{
    "name": "max107/upload-bundle",
    "license": "MIT",
    "description": "Upload Bundle with flysystem integration",
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "liip/imagine-bundle": "^2.0",
        "symfony/browser-kit": "~4.0",
        "symfony/var-dumper": "~4.0",
        "symfony/css-selector": "~4.0"
    },
    "require": {
        "oneup/flysystem-bundle": "~3.0",
        "symfony/property-access": "~4.0",
        "symfony/form": "~4.0",
        "doctrine/orm": "~2.0",
        "doctrine/doctrine-bundle": "~1.0",
        "jms/metadata": "~1.0",
        "symfony/twig-bundle": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Max107\\Bundle\\UploadBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Max107\\Bundle\\UploadBundle\\Tests\\": "./tests/",
            "Max107\\TestBundle\\": "tests/Fixtures/App/src/TestBundle/"
        }
    }
