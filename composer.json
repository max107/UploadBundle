{
  "name": "max107/upload-bundle",
  "license": "MIT",
  "description": "Upload Bundle with flysystem integration",
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "matthiasnoback/symfony-dependency-injection-test": "^4.0",
    "symfony/browser-kit": "~3.0|~4.0",
    "symfony/var-dumper": "~3.0|~4.0",
    "symfony/css-selector": "~3.0|~4.0"
  },
  "require": {
    "oneup/flysystem-bundle": "~3.0",
    "symfony/property-access": "~3.0|~4.0",
    "symfony/form": "~3.0|~4.0",
    "doctrine/orm": "~2.0",
    "doctrine/doctrine-bundle": "~1.0",
    "jms/metadata": "~1.0",
    "symfony/twig-bundle": "~3.0|~4.0",
    "liip/imagine-bundle": "~1.0|~2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Max107\\Bundle\\UploadBundle\\Tests\\": "./Tests/",
      "Max107\\TestBundle\\": "./Tests/Fixtures/App/src/TestBundle/"
    }
  },
  "autoload": {
    "psr-4": {
      "Max107\\Bundle\\UploadBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
